{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nutscrapi/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nutscrapi/tsconfig.app.json",
    "plugins": [
      "@nestjs/swagger"
    ]
  },
  "monorepo": true,
  "root": "apps/nutscrapi",
  "projects": {
    "nutscrapedb": {
      "type": "library",
      "root": "libs/nutscrapedb",
      "entryFile": "index",
      "sourceRoot": "libs/nutscrapedb/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nutscrapedb/tsconfig.lib.json"
      }
    },
    "nutscrapejob": {
      "type": "application",
      "root": "apps/nutscrapejob",
      "entryFile": "main",
      "sourceRoot": "apps/nutscrapejob/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nutscrapejob/tsconfig.app.json"
      }
    },
    "nutscraper": {
      "type": "application",
      "root": "apps/nutscraper",
      "entryFile": "main",
      "sourceRoot": "apps/nutscraper/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nutscraper/tsconfig.app.json"
      }
    },
    "nutscrapi": {
      "type": "application",
      "root": "apps/nutscrapi",
      "entryFile": "main",
      "sourceRoot": "apps/nutscrapi/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nutscrapi/tsconfig.app.json"
      }
    }
  }
}